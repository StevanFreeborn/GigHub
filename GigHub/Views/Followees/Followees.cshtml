@model IEnumerable<GigHub.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Artists</h2>
<ul class="followees">
    @foreach(var artist in Model)
    {
        <li>@artist.Name (<a href="mailto:@artist.Email">@artist.Email</a>)</li>
    }
</ul>

